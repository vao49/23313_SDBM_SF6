<form class="d-inline" method="post" action="{{ path('app_continent_delete', {'id': continent.id}) }}" onsubmit="return confirm('{% trans %}Are you sure you want to delete this item?{% endtrans %}');">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ continent.id) }}">
	<button class="btn btn-danger btn-sm {% trans %}icon delete{% endtrans %}">{% trans %}Delete{% endtrans %}</button>
</form>
