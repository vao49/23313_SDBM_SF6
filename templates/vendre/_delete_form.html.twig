<form class="d-inline" method="post" action="{{ path('app_vendre_delete', {'annee': annee,'no': no,'id': vendre.article.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ vendre.annee) }}">
    <button class="btn btn-danger  btn-sm {% trans %}icon delete{% endtrans %}">{% trans %}Delete{% endtrans %}</button>
</form>

 
